<odoo>
    <data>
        <record id="prescription_report_paperformat" model="report.paperformat">
            <field name="name">prescription Report</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">180</field>
            <field name="page_width">130</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">20</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">3</field>
            <field name="margin_right">1</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">15</field>
            <field name="dpi">90</field>
        </record>
        <report
                id="report_pharmacy_print"
                model="medical.pharmacy"
                string="Pharmcy Cash Bill"
                name="medical_pharmacy.pharmacy_print_template_call"
                file="medical_pharmacy.pharmacy_print_template_call"
                report_type="qweb-pdf"/>
        <report
                id="print_prescription_report"
                model="medical.pharmacy"
                string="Print Prescription"
                name="medical_pharmacy.print_prescription_report_template"
                file="medical_pharmacy.print_prescription_report_template"
                report_type="qweb-pdf"
                menu="False"
                paperformat="prescription_report_paperformat"/>/>
    </data>
</odoo>
